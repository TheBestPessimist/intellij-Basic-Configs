<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Compare Files">
        <Item>
          <IdeaActionID>CompareTwoFiles</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Compare Files</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>Vcs.Log.Refresh</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Refresh</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Local Changes as UML">
        <Item>
          <IdeaActionID>UML.ShowChanges</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+D'</ShortCut>
          <Description>Show Local Changes as UML</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unshelve...">
        <Item>
          <IdeaActionID>ShelveChanges.UnshelveWithDialog</IdeaActionID>
          <ShortCut>'Ctrl+Shift+U'</ShortCut>
          <Description>Unshelve...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>