<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Collapse All">
        <Item>
          <IdeaActionID>ExternalSystem.CollapseAll</IdeaActionID>
          <ShortCut>'Ctrl+NumPad -' or 'Ctrl+Minus'</ShortCut>
          <Description>Collapse All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit Message History">
        <Item>
          <IdeaActionID>Vcs.ShowMessageHistory</IdeaActionID>
          <ShortCut>'Ctrl+M' or 'Ctrl+E'</ShortCut>
          <Description>Commit Message History</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Compare Files">
        <Item>
          <IdeaActionID>CompareTwoFiles</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Compare Files</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Reference">
        <Item>
          <IdeaActionID>CopyReference</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+C'</ShortCut>
          <Description>Copy Reference</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Expand All">
        <Item>
          <IdeaActionID>ExternalSystem.ExpandAll</IdeaActionID>
          <ShortCut>'Ctrl+NumPad +' or 'Ctrl+='</ShortCut>
          <Description>Expand All</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Refresh">
        <Item>
          <IdeaActionID>Vcs.Log.Refresh</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Refresh</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'Alt+4'</ShortCut>
          <Description>Run</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>ChangesView.Diff</IdeaActionID>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Local Changes as UML">
        <Item>
          <IdeaActionID>UML.ShowChanges</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+D'</ShortCut>
          <Description>Show Local Changes as UML</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'Alt+6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unshelve...">
        <Item>
          <IdeaActionID>ShelveChanges.UnshelveWithDialog</IdeaActionID>
          <ShortCut>'Ctrl+Shift+U'</ShortCut>
          <Description>Unshelve...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9' or 'Alt+Shift+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>13</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>